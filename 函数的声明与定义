#define  _CRT_SECURE_NO_WARNINGS 1
//void代表这个函数不需要返回
#include <stdio.h>
int Add(int x, int y);//函数的声明
int main()
{
	int a, b;
	scanf("%d %d", &a, &b);
	int sum = Add(a, b);//先声明 后使用
	printf("%d\n", sum);
	return 0;
}
//函数的定义
int Add(int x, int y)
{
	return x + y;//意思是Add函数的返回值是（x+y）调用该函数之后就可以得到x+y的结果
}