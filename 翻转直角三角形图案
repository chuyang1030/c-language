#include <stdio.h>

int main()
{
    int i, j;
    int n = 0;
    while (scanf("%d", &n) == 1)
    {
        for (i = 0; i < n; i++)
        {
            for (j = 0; j<n-i; j++)//理解含义
            {
                printf("* ");
            }
            printf("\n");
        }
    }
    return 0;
}